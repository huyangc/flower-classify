train_net: "models/caffenet/train_self_part.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 20
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
max_iter: 100000
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "alexnet"
#debug_info: true